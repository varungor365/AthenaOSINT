[Unit]
Description=AthenaOSINT Web Service
After=network.target

[Service]
Type=simple
User=root
WorkingDirectory=/root/AthenaOSINT
Environment="PATH=/root/AthenaOSINT/venv/bin"
ExecStart=/root/AthenaOSINT/venv/bin/gunicorn -w 4 -b 127.0.0.1:5000 --timeout 120 run_web:app
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target
